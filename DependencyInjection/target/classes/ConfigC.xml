<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="personalInfoC" class="com.bridgelab.diUsingConstructor.PersonalInfoC">
        <constructor-arg name="firstName" value="Amit"/>
        <constructor-arg name="lastName" value="Haral"/>
        <constructor-arg name="id" value="1"/>
    </bean>

    <bean id="personalInfoC2" class="com.bridgelab.diUsingConstructor.PersonalInfoC">
        <constructor-arg name="firstName" value="Steve"/>
        <constructor-arg name="lastName" value="Jobs"/>
        <constructor-arg name="id" value="2"/>
    </bean>

    <bean id="personalInfoC3" class="com.bridgelab.diUsingConstructor.PersonalInfoC">
        <constructor-arg name="firstName" value="Mark"/>
        <constructor-arg name="lastName" value="Zuckerberg"/>
        <constructor-arg name="id" value="3"/>
    </bean>

    <bean id="personalInfoC4" class="com.bridgelab.diUsingConstructor.PersonalInfoC">
        <constructor-arg name="firstName" value="Ganesh"/>
    </bean>

</beans>